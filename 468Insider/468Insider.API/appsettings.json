{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "468InsiderDB": "Server=localhost\\SA;Database=468Insider;Trusted_Connection=True; TrustServerCertificate=True"
  },
  "Serilog" : {
    "Minimumlevel": {
      "Default": "Error"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log~.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "10"
        }
      }
    ] 
  }
  
}
